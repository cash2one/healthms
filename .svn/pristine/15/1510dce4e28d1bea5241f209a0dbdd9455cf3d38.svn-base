.healthms-vertical-center {
  height: 34px;
  line-height: 34px;
}
.healthms-width-auto {
  width: auto;
}
.healthms-clean-padding-right {
  padding-right: 0; 
}
.healthms-clean-padding-left {
  padding-left: 0; 
}
.healthms-clean-padding-right-left-5 {
  padding-right: 5px !important; 
  padding-left: 5px !important;
}
.healthms-text-white {
  color: white;
}
.healthms-container-height {
  padding-top: 20px;
}
.healthms-nav {
  margin-bottom: 0px;
}
.healthms-left-btn {
  margin-right: 30px;
}

.healthms-fill-width {
  width: 100%;
}
.healthms-gray {
  background-color: #eeeeee;
}

.healthms-login-form-title {
  margin-bottom: 30px;
}

.healthms-login-container div div ul li {
  width: 50%;
}

.healthms-login-container div div div div div div form {
  padding-top: 30px;
}

.healthms-login-panel {
  border-top-width: 0px !important;
  border-top-left-radius: 0 !important;
  border-top-right-radius: 0 !important;
}

/*
 * Footer
 */
.healthms-login-footer {
  height: 111px;
  padding: 40px 0;
  color: #fff;
  text-align: center;
  background-color: #337ab7;
  border-top: 1px solid #e5e5e5;
}

.healthms-inline {
  display: inline !important;
  width: auto !important;
}


.healthms-reg-main {
  margin-top: 50px;
  margin-bottom: 80px;
}

.healthms-reg-title {
  margin-bottom: 30px;
}

.healthms-reg-emsg {
  padding-top: 4px;
}
.healthms-patient-reg-codeimg-box {
