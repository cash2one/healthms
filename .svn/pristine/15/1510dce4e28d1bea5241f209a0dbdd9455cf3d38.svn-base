}

.healthms-index-main {
  border-left: 1px solid #eee;
  margin-bottom: 20px;
}

.healthms-patient-index-sidebar {
  margin-top: 30px;
}

.healthms-test-list-td {
  padding: 0 !important;
}

.healthms-test-list-container {
  width: 100%;
}

.healthms-test-list-radio {
  width: 20%
}

.healthms-patient-test-result-dl>dd {
